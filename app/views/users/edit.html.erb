<div class="block">
           
  <div class="content">            
    <h2 class="title"><%= t("web-app-theme.edit", :default => "Edit") %> User</h2>
    <div class="inner">
      <%= form_for @user, :url => user_path(@user), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>